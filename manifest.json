{
    "manifest_version": 3,
    "name": "TabTrail",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/icon.png"
      }
    },
    "host_permissions": ["http://*/*", "https://*/*"],
    "permissions": [
      "tabGroups",
      "tabs",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  